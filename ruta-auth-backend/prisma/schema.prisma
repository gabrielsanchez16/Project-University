datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         String   @id @default(uuid())
  nombre     String?
  apellido   String?
  correo     String   @unique
  contrasena String
  perfil_id  String?
  createdAt  DateTime @default(now())
}
